<script>
  <% class_sel = ".#{sing}-input" %>
  <% address = "/autocomplete/#{sing}.json" %>
  $(function(){
    $('<%= class_sel %>').tokenInput('<%= address %>', {
      tokenValue: 'name',
      preventDuplicates: true,
      <% if defined? is_sing %>
        tokenLimit: 1,
      <% end %>
      prePopulate: [
        <% coll.each do |obj| %>
          {name: "<%= obj.name %>"},
        <% end %>
      ]
    });
  });
</script>
