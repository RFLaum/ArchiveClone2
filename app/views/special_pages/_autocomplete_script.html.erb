<script>
  <% class_sel = ".#{sing}-input" %>
  <% address = ac_path(sing, format: :json) %>
  $(function(){
    $('<%= class_sel %>').tokenInput('<%= address %>', {
      tokenValue: 'name',
      preventDuplicates: true,
      <% if defined? is_sing %>
        tokenLimit: 1,
      <% end %>
      prePopulate: [
        <% coll.each do |obj| %>
          {name: "<%= obj.is_a?(String) ? obj : obj.name %>"},
        <% end %>
      ]
    });
  });
</script>
